local Button = MainTab:CreateButton({
   Name = "Lagger Caution[Client Side]",
   Callback = function()
   -- âš  GUI Lag Dropper (LocalScript) - Use with caution!
-- âœ… Works with Delta, Solara, Fluxus

-- SETTINGS
local decalId = "rbxassetid://1234567890" -- Replace with your decal ID
local lagAmount = 200 -- Parts per spam
local delay = 3 -- Seconds between spam

-- GUI Creation
local ScreenGui = Instance.new("ScreenGui", game.CoreGui)
local Button = Instance.new("TextButton", ScreenGui)

ScreenGui.Name = "LagDropperGUI"
Button.Size = UDim2.new(0, 150, 0, 50)
Button.Position = UDim2.new(0.5, -75, 0.8, 0)
Button.Text = "Start Lag Dropper"
Button.BackgroundColor3 = Color3.fromRGB(255, 50, 50)
Button.TextColor3 = Color3.new(1, 1, 1)
Button.Font = Enum.Font.SourceSansBold
Button.TextSize = 20
Button.BackgroundTransparency = 0.2
Button.BorderSizePixel = 3

-- Variables
local running = false

-- Lag Function
local function dropLagParts()
    for i = 1, lagAmount do
        local p = Instance.new("Part", workspace)
        p.Size, p.Anchored, p.CanCollide, p.Material = Vector3.new(6, 6, 1), false, true, Enum.Material.Neon
        p.Position = Vector3.new(math.random(-500, 500), math.random(150, 300), math.random(-500, 500))
        for _, face in ipairs(Enum.NormalId:GetEnumItems()) do
            local d = Instance.new("Decal", p)
            d.Texture, d.Face = decalId, face
        end
    end
end

-- Toggle Button
Button.MouseButton1Click:Connect(function()
    running = not running
    Button.Text = running and "Stop Lag Dropper" or "Start Lag Dropper"
    Button.BackgroundColor3 = running and Color3.fromRGB(50, 200, 50) or Color3.fromRGB(255, 50, 50)

    if running then
        task.spawn(function()
            while running do
                dropLagParts()
                wait(delay)
            end
        end)
    end
end)
   end,
})

